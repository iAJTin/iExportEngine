<?xml version="1.0" encoding="utf-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           version="1.0">

  <!-- ====================================== -->
  <!-- Definition of mini-chart data types    -->
  <!-- ====================================== -->

  <!-- Static binding data type: Defines a static resource property, represents a call to external assembly. -->
  <xs:simpleType id="staticBindType" name="staticBindType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*\{\s*[S|s]tatic[B|b]inding\s*:\s*@?_*[A-Za-z]{1}[A-Za-z0-9]*\s*\}"/>
      <xs:pattern value="\s*\{\s*[S|s]tatic[B|b]inding\s*:\s*(@?_*[A-Za-z]{1}[A-Za-z0-9]*)\.?@?_*[A-Za-z]{1}[A-Za-z0-9]*\s*\}"/>
      <xs:whiteSpace value="collapse" />
    </xs:restriction>
  </xs:simpleType>

  <!-- ShortCut: StaticBinding. -->
  <xs:simpleType name="shortCutStaticBindingType" id="shortCutStaticBindingType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="{StaticBinding: }">
        <xs:annotation>
          <xs:documentation>Represents a external method or property call.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:whiteSpace value="collapse"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- miniChartEmptyValuesAsEnumType: Represents a list of known mini-chart empty values as enumeration -->
  <xs:simpleType name="miniChartEmptyValuesAsEnumType" id="miniChartEmptyValuesAsEnumType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Gap">
        <xs:annotation>
          <xs:documentation>Custom </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Zero">
        <xs:annotation>
          <xs:documentation>Defines </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Connect">
        <xs:annotation>
          <xs:documentation>Defines </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:whiteSpace value="collapse"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- miniChartAxisColorEnumType: Represents a list of known mini-chart empty values as enumeration -->
  <xs:simpleType name="miniChartHorizontalAxisColorEnum" id="miniChartHorizontalAxisColorEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Automatic">
        <xs:annotation>
          <xs:documentation>Automatic color</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:whiteSpace value="collapse"/>
    </xs:restriction>
  </xs:simpleType>

