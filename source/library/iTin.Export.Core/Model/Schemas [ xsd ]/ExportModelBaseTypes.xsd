  <!-- ================================== -->
  <!-- Definition of new basic data types -->
  <!-- ================================== -->
  
  <!-- Boolean data type -->
  <xs:simpleType name="yesNoType">
    <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
      <xs:documentation xml:lang="en">
        Represents a boolean value
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="Yes">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Represents the boolean value 'true'
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="No">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Represents the boolean value 'false'
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- String data type. Defines an empty string. -->
  <xs:simpleType id="emptyStringType" name="emptyStringType">
    <xs:restriction base="xs:string">
      <xs:pattern value=""/>
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Integer data type. Represents an integer, the minimum allowed value is 1. -->
  <xs:simpleType name='intMinValueIsOneType'>
    <xs:restriction base='xs:integer'>
      <xs:minInclusive value='1'/>
    </xs:restriction>
  </xs:simpleType>

  <!-- Integer data type. Represents an integer, the minimum allowed value is -1. -->
  <xs:simpleType name='intMinValueIsMinusOneType'>
    <xs:restriction base='xs:integer'>
      <xs:minInclusive value='-1'/>
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Integer array. Defines an integer array where each element is greater or equal to 1. -->
  <xs:simpleType name="integerArrayWhereMinValueIsOneType">
    <xs:list itemType="intMinValueIsOneType"/>
  </xs:simpleType>

  <!-- Integer array. Defines an integer array where each element is greater or equal to -1. -->
  <xs:simpleType name="integerArrayWhereMinValueIsMinusOneType">
    <xs:list itemType="intMinValueIsMinusOneType"/>
  </xs:simpleType>

  <!-- Bind data type: Defines a binding property, represents a call to external assembly. -->
  <xs:simpleType id="bindingType" name="bindingType">
    <xs:restriction base="xs:string">
      <xs:pattern value="\s*\{\s*[B|b]inding\s*:\s*@?_*[A-Za-z]{1}[A-Za-z0-9]*\s*\}"/>
      <xs:pattern value="\s*\{\s*[B|b]inding\s*:\s*(@?_*[A-Za-z]{1}[A-Za-z0-9]*)\.?@?_*[A-Za-z]{1}[A-Za-z0-9]*\s*\}"/>
      <xs:whiteSpace value="collapse" />
    </xs:restriction>
  </xs:simpleType>


  <!-- =============================== -->
  <!-- Definition of string data types -->
  <!-- =============================== -->

  <!-- Path. Represents a path -->
  <xs:simpleType name="pathType">
    <xs:restriction base="xs:string">
      <xs:pattern value="^(.*/)?(?:$|(.+?)(?:(\.[^.]*$)|$))"/>
      <xs:whiteSpace value="collapse"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- String array. Defines a string array. -->
  <xs:simpleType name="stringArrayType">
    <xs:list itemType="xs:string"/>
  </xs:simpleType>

  
  <!-- ================================ -->
  <!-- Definition of drawing data types -->
  <!-- ================================ -->

  <!-- RGB color. Expressed in hexadecimal notation (#RRGGBB). -->
  <xs:simpleType name="rgbHexColorType" id ="rgbHexColorType">
    <xs:restriction base="xs:string">
      <xs:pattern value="#[0-9a-fA-F]{6}"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- RGB color. Defined by a value representing a % for each RGB component. -->
  <xs:simpleType name="scColorType" id ="scColorType">
    <xs:restriction base="xs:string">
      <xs:pattern value ="sc#\W\d+.\d+\W\d+.\d+\W\d+.\d+"/>
    </xs:restriction>
  </xs:simpleType>
  
