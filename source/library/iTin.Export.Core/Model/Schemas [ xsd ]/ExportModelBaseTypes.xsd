  <!-- identifier Type: Represents a valid name for an identifier. -->
  <xs:simpleType id="identifierType" name="identifierType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[\w%@#-_]+" />
      <xs:whiteSpace value="collapse" />
    </xs:restriction>
  </xs:simpleType>


  <!-- ====================================== -->
  <!-- Definition of condition operators      -->
  <!-- ====================================== -->

  <!-- Operator. Operator names -->
  <xs:simpleType name="operatorNameType">
    <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
      <xs:documentation xml:lang="en">
        List of available operators
      </xs:documentation>
    </xs:annotation>
      <xs:restriction base="xs:string">
        <xs:enumeration value="EqualTo">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value is equals to.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="NotEqualTo">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value is not equals to.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="LessThan">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value is less than.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="LessOrEqualThan">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value is less or equal than.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="GreatherThan">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value is greather than.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="GreatherOrEqualsThan">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value is greather or equal than.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="In">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value into value-list.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="Like">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value select into match pattern.
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="Beetween">
          <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
            <xs:documentation xml:lang="en">
              Value into two values
            </xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
  </xs:simpleType>

  <!-- Short cut: Operators -->
  <xs:simpleType name="operatorType" id ="operatorType">
    <xs:union memberTypes="shortCutStaticBindingType operatorNameType"/>
  </xs:simpleType>

</xs:schema>
