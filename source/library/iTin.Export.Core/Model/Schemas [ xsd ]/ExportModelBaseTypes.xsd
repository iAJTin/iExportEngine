  <!-- Mail address. Represents a mail address. -->
  <xs:simpleType name="mailAddressType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[A-Za-z0-9_]+([-+][A-Za-z0-9_]+)*@[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*\.[A-Za-z0-9_]+([-.][A-Za-z0-9_]+)*" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Address list. Represents a mail address list. -->
  <xs:simpleType name="mailAddressListType">
    <xs:list itemType="mailAddressType" />
  </xs:simpleType>

  <!-- IP Address. Represents an ip address, range 1.0.0.0 to 255.255.255.255 -->
  <xs:simpleType id="ipAddressType" name="ipAddressType">
    <xs:restriction base="xs:string">
      <xs:pattern value="^([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(\.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3}$"/>
    </xs:restriction>
  </xs:simpleType>

