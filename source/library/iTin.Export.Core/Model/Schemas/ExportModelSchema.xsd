            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Top" type="xs:double" use="optional" default="20">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
